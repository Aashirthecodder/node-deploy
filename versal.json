{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@versal/node"
    },
    {
      "src": "build/**",
      "use": "@versal/static"
    }
  ],
  "routes": [
    {
      "src": "/products",

      "dest": "index.js"
    },
    {
      "src": "/products/(.*)",

      "dest": "index.js"
    },
    {
      "src": "/",

      "dest": "build/index.html"
    },
    {
      "src": "/(.+)",

      "dest": "build/$1"
    }
  ]
}
